<h4>Commits</h4>
<table class="table table-striped">
<tbody>
	{{#each}}
		<tr>
			<td><img {{bind-attr src="author.avatar_url"}} style="width:100px;" /></td>
			<td>
			<h5>{{sha}}</h5>
			<p>{{commit.message}}</p>
			<hr />
			{{fromDate created_at}} </td>
		</tr>
	{{else}}
		<tr><td><p class="text-muted">No commits yet!</p></td></tr>	
	{{/each}}
</tbody>
</table>